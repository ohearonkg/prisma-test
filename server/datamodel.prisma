enum ExerciseType {
  BODYWEIGHT
  ASSISTED
  WEIGHTED
  CARDIO
}

type User {
  id: ID! @unique
  name: String!
  programs: [Program]!
}

type Program {
  id: ID! @unique
  name: String!
  user: User!
}

type Exercise {
  id: ID! @unique
  name: String!
  exerciseType: ExerciseType!
  description: String
}
