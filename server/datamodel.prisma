enum ExerciseType {
  BODYWEIGHT
  ASSISTED
  WEIGHTED
  CARDIO
}

type User {
  id: ID! @unique
  username: String! @unique
  password: String!
  programs: [Program]!
}

type Program {
  id: ID! @unique
  name: String!
  user: User!
  exercises: [Exercise]!
}

type Exercise {
  id: ID! @unique
  name: String!
  type: ExerciseType!
  description: String
  Program: [Program]!
}
